#include<stdio.h>
int main()
{
int a,smallest,largest,count=0;
printf("How many numbers do you want:");
scanf("%d",&count);
printf("Enter number 1:");
scanf("%d",&largest);
while(count>=0)
{
 printf("Enter number %d:");
 scanf("%d",&a);
 if(a>largest){
 a=largest;
 printf("The largest number is:%d\n",a);
 }else if(a<smallest){
 a=smallest;
 printf("The smallest number is:%d\n",a);
 }
}
count++;
return 0;
}
