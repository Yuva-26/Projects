#include<stdio.h>
#include<conio.h>
int main()
{
int i,j,k,octal;
printf("Enter i:");
scanf("%d",&i);
printf("Enter j:");
scanf("%d",&j);
printf("Enter k:");
scanf("%d",&k);
printf("Integer:%d%d%d\n",i,j,k);
if((i<=7)&&(j<=7)&&(k<=7))

   {
    octal=(i*(8*8))+(j*(8*1))+(k*1);
    printf("Octal equivalent:%d",octal);
   }
else
    printf("Error\n");
getch();
}
